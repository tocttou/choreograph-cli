{
  "name": "choreograph",
  "version": "1.0.0",
  "description": "An OSS tool to orchestrate docker based workers. Comes with a web-UI and an API.",
  "main": "dist/bundle.js",
  "bin" :{
    "choreo": "dist/bundle.js"
  },
  "scripts": {
    "build": "npm-run-all lint:build clean-dist && node_modules/.bin/babel src/index.js --out-file dist/bundle.js",
    "dev": "npm-run-all lint && babel-node src/index.js",
    "start": "node dist/bundle.js",
    "lint": "eslint webpack.config.js src",
    "lint:build": "eslint webpack.config.js src -c ./.eslintrc-build",
    "lint:fix": "eslint src webpack.config.js --fix",
    "clean-dist": "node_modules/.bin/rimraf ./dist && mkdir dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Docker",
    "worker",
    "choreograph"
  ],
  "author": "Ashish <me@ashishchaudhary.in> (https://ashishchaudhary.in)",
  "license": "MIT",
  "dependencies": {
    "byline": "^5.0.0",
    "dockerode": "^2.3.0",
    "js-yaml": "^3.6.1",
    "superagent": "^2.2.0",
    "vorpal": "1.11.4"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "eslint": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "npm-run-all": "^2.3.0",
    "json-loader": "^0.5.4",
    "rimraf": "^2.5.4",
    "webpack": "^2.1.0-beta.20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tocttou/choreograph-cli"
  }
}
